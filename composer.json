{
    "name": "snipershady/securecipher",
    "description": "A free, reliable and easy-to-use cipher to encrypt and decrypt strings",
    "keywords": ["securecipher", "iv", "encrypt", "decrypt", "cipher", "cryptography"],
    "homepage": "https://www.spinfo.it",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Stefano Perrini",
            "email": "perrini.stefano@gmail.com",
            "homepage": "https://www.spinfo.it"
        }
    ],
    "autoload": {
        "psr-4": {
            "SecureCipher\\": "src/"
        }       
    },
    "autoload-dev": {
        "psr-4": {
            "SecureCipher\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2",
        "phpstan/phpstan": "^1.10"
    }
}
